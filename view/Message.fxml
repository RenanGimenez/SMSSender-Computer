<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.TextFlow?>

<HBox fillHeight="false" minWidth="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MyController">
   <styleClass>
      <String fx:value="hbox" />
      <String fx:value="sent" />
   </styleClass>
   <children>
      <TextFlow maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="75.0" prefWidth="500.0" textAlignment="JUSTIFY">
         <styleClass>
            <String fx:value="message" />
            <String fx:value="sent" />
         </styleClass>
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </TextFlow>
   </children>
</HBox>
