<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fx:id="messagesScrollPane" fitToHeight="true" fitToWidth="true" minWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"  fx:controller="controller.MyController">
   <content>
      <VBox fx:id="messagesVBox" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button id="displayOldMessages" mnemonicParsing="false" prefHeight="50.0" prefWidth="1920.0" onAction="#displayOldMessages" text="Display old messages...">
               <font>
                  <Font size="26.0" />
               </font>
            </Button>
         </children></VBox>
   </content>
</ScrollPane>
